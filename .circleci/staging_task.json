{
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/applications/governance",
          "awslogs-region": "us-east-2"
        }
      },
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "cpu": 15,
      "memory": 1000,
      "image": "511132117039.dkr.ecr.us-east-2.amazonaws.com/governance:latest",
      "essential": true,
      "name": "governance"
    }
  ],
  "family": "staging-currency-governance-srv",
  "networkMode": "awsvpc",
  "volumes": [
    {
      "name": "service-storage",
      "host": {
        "sourcePath": "/ecs/service-storage"
      }
    }
  ]
}
